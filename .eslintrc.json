{
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "airbnb-typescript",
    "prettier",
    "plugin:storybook/recommended"
  ],
  "rules": {
    "react/jsx-props-no-spreading": 0,
    "react/jsx-uses-react": 0,
    "@typescript-eslint/no-useless-constructor": 0,
    "no-plusplus": 0,
    "react/react-in-jsx-scope": 0
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  }
}
